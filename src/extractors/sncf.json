[
    {
        "type": "pdf",
        "filter": [
            { "header": "From", "match": "e-billet@sncf.fr" },
            { "type": "Barcode", "match": "^i0CV" }
        ],
        "script": "sncf.js",
        "function": "parsePdf"
    },
    {
        "type": "text",
        "filter": [ { "header": "From", "match": "e-billet@sncf.fr" } ],
        "script": "sncf.js",
        "function": "parseText"
    },
    {
        "type": "html",
        "filter": [ { "header": "From", "match": "@oui.sncf" } ],
        "script": "sncf.js",
        "function": "parseHtmlConfirmation"
    }
]
