[
    {
        "filter": [
            {
                "field": "From",
                "match": "e-billet@sncf.fr",
                "type": "Email",
                "scope": "Ancestors"
            },
            {
                "match": "^i0CV",
                "type": "Barcode",
                "scope": "Descendants"
            }
        ],
        "function": "parsePdf",
        "script": "sncf.js",
        "type": "Pdf"
    },
    {
        "filter": [
            {
                "match": "^i[01]CV",
                "type": "Text"
            }
        ],
        "function": "parseSncfBarcode",
        "script": "sncf.js",
        "type": "Text"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "UNITTEST-e-billet@sncf.fr",
                "type": "Email",
                "scope": "Ancestors"
            }
        ],
        "function": "parseSncfPdfText",
        "script": "sncf.js",
        "type": "Text"
    },
    {
        "filter": [
            {
                "match": "^2200",
                "type": "Barcode",
                "scope": "Descendants"
            }
        ],
        "function": "parseSecutixPdf",
        "script": "sncf.js",
        "type": "Pdf"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "oui.sncf",
                "type": "Email",
                "scope": "Ancestors"
            }
        ],
        "function": "parseOuigoEmail",
        "script": "sncf.js",
        "type": "Html"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "ne-pas-repondre@pasngr.ouigo.com",
                "type": "Email",
                "scope": "Ancestors"
            }
        ],
        "function": "parseOuigoTicket",
        "script": "sncf.js",
        "type": "Pdf"
    }
]
