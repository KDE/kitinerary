{
    "filter": [
        {
            "field": "Subject",
            "match": "^\\[Indico\\] ",
            "mimeType": "message/rfc822",
            "scope": "Ancestors"
        },
        {
            "field": "Subject",
            "match": "^Registration modified ",
            "mimeType": "message/rfc822",
            "scope": "Ancestors"
        }
    ],
    "function": "parseRegistration",
    "script": "indico.js",
    "mimeType": "text/html"
}
