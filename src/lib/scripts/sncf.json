[
    {
        "filter": [
            {
                "field": "From",
                "match": "e-billet@sncf.fr",
                "mimeType": "message/rfc822",
                "scope": "Ancestors"
            },
            {
                "match": "^i0CV",
                "mimeType": "text/plain",
                "scope": "Descendants"
            }
        ],
        "function": "parsePdf",
        "script": "sncf.js",
        "mimeType": "application/pdf"
    },
    {
        "filter": [
            {
                "match": "^i[01]CV",
                "mimeType": "text/plain"
            }
        ],
        "function": "parseSncfBarcode",
        "script": "sncf.js",
        "mimeType": "text/plain"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "UNITTEST-e-billet@sncf.fr",
                "mimeType": "message/rfc822",
                "scope": "Ancestors"
            }
        ],
        "function": "parseSncfPdfText",
        "script": "sncf.js",
        "mimeType": "text/plain"
    },
    {
        "filter": [
            {
                "match": "^2200",
                "mimeType": "application/octet-stream",
                "scope": "Descendants"
            }
        ],
        "function": "parseSecutixPdf",
        "script": "sncf.js",
        "mimeType": "application/pdf"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "oui.sncf",
                "mimeType": "message/rfc822",
                "scope": "Ancestors"
            }
        ],
        "function": "parseOuigoEmail",
        "script": "sncf.js",
        "mimeType": "text/html"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "ne-pas-repondre@pasngr.ouigo.com",
                "mimeType": "message/rfc822",
                "scope": "Ancestors"
            }
        ],
        "function": "parseOuigoTicket",
        "script": "sncf.js",
        "mimeType": "application/pdf"
    }
]
