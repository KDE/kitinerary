[
    {
        "filter": [
            {
                "field": "From",
                "match": "e-billet@sncf.fr",
                "mimeType": "message/rfc822",
                "scope": "Ancestors"
            },
            {
                "match": "^i0CV",
                "mimeType": "text/plain",
                "scope": "Descendants"
            }
        ],
        "function": "parsePdf",
        "mimeType": "application/pdf",
        "script": "sncf.js"
    },
    {
        "filter": [
            {
                "match": "^i[01]CV",
                "mimeType": "text/plain",
                "scope": "Current"
            }
        ],
        "function": "parseSncfBarcode",
        "mimeType": "text/plain",
        "script": "sncf.js"
    },
    {
        "filter": [
            {
                "field": "From",
                "match": "UNITTEST-e-billet@sncf.fr",
                "mimeType": "message/rfc822",
                "scope": "Ancestors"
            }
        ],
        "function": "parseSncfPdfText",
        "mimeType": "text/plain",
        "script": "sncf.js"
    },
    {
        "filter": [
            {
                "match": "^2200",
