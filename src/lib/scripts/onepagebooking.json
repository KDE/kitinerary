[{
    "filter": [
        {
            "field": "From",
            "match": "@onepagebooking.com",
            "mimeType": "message/rfc822",
            "scope": "Ancestors"
        }
    ],
    "function": "main",
    "mimeType": "text/plain",
    "script": "onepagebooking.js"
},
{
    "filter": [
        {
            "field": "passTypeIdentifier",
            "match": "pass.onepagebooking.com",
            "mimeType": "application/vnd.apple.pkpass",
            "scope": "Current"
        }
    ],
    "function": "parsePkPass",
    "mimeType": "application/vnd.apple.pkpass",
    "script": "onepagebooking.js"
}]
