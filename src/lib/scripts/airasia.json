[{
    "filter": [
        {
            "field": "From",
            "match": "itinerary@booking.airasia.com",
            "mimeType": "message/rfc822",
            "scope": "Ancestors"
        }
    ],
    "function": "extractBookingConfirmation",
    "mimeType": "text/html",
    "script": "airasia.js"
},
{
    "filter": [
        {
            "field": "title",
            "match": "^PDF Itinerary$",
            "mimeType": "application/pdf",
            "scope": "Current"
        }
    ],
    "function": "extractItinerary",
    "mimeType": "application/pdf",
    "script": "airasia.js"
}]
